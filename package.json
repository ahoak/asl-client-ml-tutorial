{
  "name": "asl-fingerspelling-tutorial",
  "version": "0.0.1",
  "description": "asl fingerspelling client ml tutorial",
  "scripts": {
    "wasm": "cp node_modules/@tensorflow/tfjs-backend-wasm/dist/tfjs-backend-wasm.wasm ./public",
    "start:wasm": "yarn wasm && vite",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prepare": "husky install",
    "prettify": "prettier \"src/**/*.{ts, json, js}\" --write --config ./.prettierrc",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@essex/eslint-config": "^20.3.5",
    "@essex/eslint-plugin": "^20.3.8",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-handlebars": "^1.6.0",
    "vite-plugin-mpa": "^1.1.4",
    "vite-plugin-string": "^1.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "engines": {
    "node": ">=14.x"
  },
  "staticFiles": {
    "staticPath": [
      "./node_modules/@tensorflow/tfjs-backend-wasm/dist"
    ],
    "excludeGlob": [
      "**/!(*.wasm)"
    ]
  },
  "dependencies": {
    "@fluentui/web-components": "^2.5.3",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/hands": "^0.4.1646424915",
    "@monaco-editor/loader": "^1.3.2",
    "@tensorflow/tfjs": "^3.19.0",
    "@tensorflow/tfjs-backend-wasm": "^3.19.0",
    "@tensorflow/tfjs-core": "^3.19.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.0",
    "monaco-editor": "^0.34.0",
    "nanoevents": "^7.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "npm run prettify",
      "npm run lint"
    ]
  }
}
